{
    "globals": [
        "REQUIRE_CONFIG",
        "require",
        "define",
        "describe",
        "beforeEach",
        "afterEach",
        "module",
        "inject",
        "spyOn",
        "expect",
        "it"
    ],

    "browser": true,
    "worker": true,

    "bitwise": false,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "maxcomplexity": 10,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": "single",
    "regexp": false,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "white": false,
    "maxparams": 7,
    "maxdepth": 4,
    "maxlen": 120,
    "eqnull": true,

    "evil": false,
    "onecase": true,
    "devel": true
}
